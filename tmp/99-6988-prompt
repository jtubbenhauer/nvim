<SELECTION_CONTENT>
local M = {
	"zbirenbaum/copilot.lua",
	dependencies = {
		"copilotlsp-nvim/copilot-lsp", -- (optional) for NES functionality
	},
	opts = {
		suggestion = {
			auto_trigger = true,
			accept = "<Tab>",
		},
	},
	cmd = "Copilot",
	event = "InsertEnter",
}

return M
</SELECTION_CONTENT>
